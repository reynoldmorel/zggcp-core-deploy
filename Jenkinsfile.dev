node {
    stage('Removing old files') {
        sh 'rm -rf zggcp'
    }

    stage('Updating new files') {
        sh 'sudo cp -r ../zggcp-core/target/mvn-repo/. /'
    }

    stage('Updating Zggcp-Core-Deploy') {
      sh 'git add .'
      sh 'git commit -m "Updated from Jenkins"'
      sh 'git push origin feature/dev'
    }
}